#!/usr/bin/env node


var inquirer = require('inquirer');

var options = {
    type: "checkbox",
    name: "dirs",
    message: 'MESSAGE',
    choices: [
      '1',
      '2',
      '3'
    ]
};

inquirer.prompt(options, function(answers) {
  console.log('*********', answers)
});

